{
  "name": "@knights-dev/node-editor",
  "version": "0.1.0",
  "scripts": {
    "codegen": "typed-scss-modules \"src/**/*.module.scss\"",
    "codegen:watch": "typed-scss-modules --watch \"src/**/*.module.scss\"",
    "bundle": "webpack",
    "serve": "webpack-dev-server --open",
    "build": "run-s codegen bundle",
    "dev": "run-p codegen:watch serve",
    "lint:ts": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "lint:ts:fix": "eslint --fix ./src/**/*.ts ./src/**/*.tsx",
    "lint:js": "prettier --check ./*.js",
    "lint:js:fix": "prettier --write ./*.js",
    "lint:style": "stylelint ./src/**/*.scss ./src/**/*.css",
    "lint:style:fix": "stylelint ./src/**/*.scss ./src/**/*.css --fix",
    "lint": "run-p lint:ts lint:js lint:style",
    "format": "run-p lint:ts:fix lint:js:fix lint:style:fix"
  },
  "description": "functional visual programming environment",
  "repository": "git@github.com:0918nobita/knights.git",
  "author": "0918nobita",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@fleur/fleur": "3.0.1",
    "@fleur/react": "5.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "@wasm-tool/wasm-pack-plugin": "1.6.0",
    "css-loader": "6.7.1",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.12",
    "prettier": "2.6.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "3.3.1",
    "stylelint": "14.6.1",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-loader": "9.2.8",
    "typed-scss-modules": "^6.2.0",
    "typescript": "4.6.3",
    "wasm-pack": "0.10.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
