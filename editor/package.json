{
  "name": "@knights-dev/node-editor",
  "version": "0.1.0",
  "scripts": {
    "codegen": "typed-scss-modules \"src/**/*.module.scss\"",
    "codegen:watch": "typed-scss-modules --watch \"src/**/*.module.scss\"",
    "vite": "vite",
    "vite:build": "vite build",
    "build": "run-s codegen vite:build",
    "dev": "run-p codegen:watch vite",
    "eslint": "eslint . --ext .ts,.tsx,.js",
    "eslint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint:fix": "stylelint ./src/**/*.scss --fix",
    "lint": "run-p eslint stylelint",
    "format": "run-p eslint:fix stylelint:fix"
  },
  "description": "functional visual programming environment",
  "repository": "git@github.com:0918nobita/knights.git",
  "author": "0918nobita",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@fleur/fleur": "3.1.0",
    "@fleur/react": "5.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@vitejs/plugin-react": "1.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.12",
    "prettier": "2.6.1",
    "sass": "1.49.11",
    "stylelint": "14.6.1",
    "stylelint-config-standard-scss": "3.0.0",
    "typed-scss-modules": "6.2.0",
    "typescript": "4.6.3",
    "vite": "2.9.1"
  }
}
